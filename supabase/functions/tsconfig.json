{
  "compilerOptions": {
    "target": "ES2022",
    "lib": [
      "ES2022",
      "WebWorker"
    ],
    "module": "ESNext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "allowJs": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": [
      "./types",
      "../node_modules/@types"
    ],
    "baseUrl": ".",
    "paths": {
      "https://esm.sh/@supabase/supabase-js@2.45.4": [
        "./types.d.ts"
      ]
    },
    "allowImportingTsExtensions": true,
    "noEmit": true
  },
  "include": [
    "**/*.ts",
    "**/*.d.ts",
    "types.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}