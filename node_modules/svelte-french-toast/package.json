{
  "name": "svelte-french-toast",
  "version": "1.2.0",
  "description": "Buttery smooth Svelte toasts. Lightweight, customizable, and beautiful by default.",
  "keywords": [
    "svelte",
    "notifications",
    "toast",
    "snackbar"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kbrgl/svelte-french-toast.git"
  },
  "license": "MIT",
  "author": "Kabir Goel",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "svelte-writable-derived": "^3.1.0"
  },
  "peerDependencies": {
    "svelte": "^3.57.0 || ^4.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.31.2",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.20.5",
    "@sveltejs/package": "^2.1.0",
    "@types/prismjs": "^1.26.0",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-svelte": "^2.31.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.10.1",
    "prism-svelte": "^0.5.0",
    "prismjs": "^1.29.0",
    "publint": "^0.1.10",
    "svelte": "^4.0.0",
    "svelte-check": "^3.4.4",
    "tailwindcss": "^3.2.7",
    "tslib": "^2.5.0",
    "typescript": "~5.0.2",
    "vite": "^4.2.0",
    "vitest": "^0.29.3"
  },
  "scripts": {
    "build": "vite build && pnpm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "format": "prettier --plugin-search-dir . --write .",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "package": "svelte-kit sync && svelte-package && publint && cp README.md header-image.png dist/",
    "preview": "vite preview",
    "test": "playwright test",
    "test:unit": "vitest"
  }
}