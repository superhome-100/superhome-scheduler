{
  "name": "app",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "cross-env ROLLUP_SKIP_NODEJS_NATIVE=1 vite build",
    "postinstall": "node ./scripts/ensure-rollup-native.cjs && node ./scripts/ensure-workerd.cjs",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json",
    "types": "npx supabase gen types typescript --local > ./src/lib/database.types.ts",
    "reset": "npx supabase db reset & npm run types",
    "migration": "npx supabase migration up --local --include-all"
  },
  "dependencies": {
    "@esbuild/darwin-arm64": "^0.27.0",
    "@fullcalendar/core": "^6.1.19",
    "@fullcalendar/daygrid": "^6.1.19",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.74.0",
    "dayjs": "^1.11.18"
  },
  "devDependencies": {
    "@sveltejs/adapter-cloudflare": "^7.2.4",
    "@sveltejs/kit": "^2.46.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/postcss": "^4.1.14",
    "@tailwindcss/typography": "^0.5.19",
    "@tsconfig/svelte": "^5.0.5",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.1.0",
    "daisyui": "^5.5.5",
    "postcss": "^8.5.6",
    "svelte": "^5.39.11",
    "svelte-check": "^4.3.3",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "vite": "^7.1.9"
  },
  "optionalDependencies": {
    "@rollup/rollup-darwin-arm64": "^4.53.3",
    "lightningcss-darwin-arm64": "^1.30.2"
  }
}
