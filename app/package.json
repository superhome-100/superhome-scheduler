{
  "name": "app",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "cross-env ROLLUP_SKIP_NODEJS_NATIVE=1 vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json",
    "types": "npx supabase gen types typescript --local > ./src/lib/database.types.ts",
    "reset": "npx supabase db reset & npm run types",
    "migration": "npx supabase migration up --local --include-all"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.19",
    "@fullcalendar/daygrid": "^6.1.19",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.4",
    "dayjs": "^1.11.13"
  },
  "devDependencies": {
    "@sveltejs/adapter-cloudflare": "^4.7.2",
    "@sveltejs/kit": "^2.6.4",
    "@sveltejs/vite-plugin-svelte": "^6.1.1",
    "@tailwindcss/typography": "^0.5.15",
    "@tsconfig/svelte": "^5.0.4",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "daisyui": "^4.12.14",
    "postcss": "^8.4.49",
    "svelte": "^5.38.1",
    "svelte-check": "^4.3.1",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.8.3",
    "vite": "^7.1.2"
  }
}
